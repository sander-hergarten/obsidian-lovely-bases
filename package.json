{
  "name": "lovely-bases",
  "version": "0.0.3",
  "description": "Beautiful, visual bases views for Obsidian",
  "main": "main.js",
  "scripts": {
    "css": "npx @tailwindcss/cli -i src/main.css -o build/lovely-bases/styles.css --watch",
    "css:build": "npx @tailwindcss/cli -i src/main.css -o build/lovely-bases/styles.css --minify",
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && npm run css:build && node scripts/scope.js",
    "dev-install": "node scripts/dev-install.mjs",
    "init": "node scripts/init.mjs",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
