    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "builtin-modules": "^5.0.0",
    "electron": "^13.6.9",
    "esbuild": "0.27.2",
    "esbuild-plugin-copy": "^2.1.1",
    "obsidian": "latest",
    "postcss": "^8.5.6",
    "postcss-prefix-selector": "^1.16.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
